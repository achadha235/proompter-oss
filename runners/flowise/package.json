{
  "name": "@proompter/runner-flowise",
  "type": "module",
  "version": "0.0.6",
  "private": false,
  "main": "./build/index.js",
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "flowise": "workspace:*",
    "flowise-components": "workspace:*",
    "typeorm": "^0.3.17",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "sqlite3": "^5.1.6"
  },
  "peerDependenciesMeta": {
    "sqlite3": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsc && npm run bundle-flowise",
    "bundle-flowise": "mkdir -p ./build/node_modules/flowise && cp ../../monorepos/flowise/packages/server/package.json ./build/node_modules/flowise && cp -r ../../monorepos/flowise/packages/server/dist ./build/node_modules/flowise",
    "dev": "tsc --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@proompter/core": "*",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "winston": "^3.11.0"
  }
}