{
  "name": "@proompter/react",
  "module": "index.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./build/index.js",
      "import": "./build/index.js",
      "default": "./build/index.js",
      "types": "./index.ts"
    },
    "./styles/global": "./build/styles/globals.css"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@happy-dom/global-registrator": "^12.10.3",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/test": "^7.6.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "^14.1.2",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.15",
    "daisyui": "^4.4.14",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.0",
    "tailwindcss": "^3.3.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "dev": "turbo run dev:js dev:css --parallel",
    "build": "bunx rimraf build/* && NODE_ENV=production bun run build:js && bun run build:css",
    "build:css": "postcss styles/globals.css styles/**/*.css --dir build/styles",
    "build:js": "tsc",
    "dev:css": "bun run build:css --watch",
    "dev:js": "bun run build:js --watch",
    "test": "bun test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@proompter/core": "workspace:*",
    "ai": "workspace:*",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.9",
    "lodash": "^4.17.21",
    "react-markdown": "^9.0.1",
    "react-textarea-autosize": "^8.5.3",
    "remark-gfm": "^4.0.0",
    "usehooks-ts": "^2.9.1"
  }
}